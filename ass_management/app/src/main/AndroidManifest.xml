<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ass_management">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".unusefulActivity"></activity>
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".EditpwActivity" />
        <activity android:name=".JoinAssActivity" />
        <activity android:name=".BuildAssActivity" />
        <activity android:name=".SubActActivity" />
        <activity android:name=".ProgressActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".EditJsActivity" />
        <activity android:name=".ProgressSecondActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".EditAsActivity" />
        <activity android:name=".JoinAssApprovalActivity" />
        <activity android:name=".ApprovalJsActivity" />
        <activity android:name=".BuildAssApprovalActivity" />
        <activity android:name=".ApprovalAsActivity" />
        <activity android:name=".SubActApprovalActivity" />
        <activity android:name=".ApprovalActActivity" />
    </application>

</manifest>